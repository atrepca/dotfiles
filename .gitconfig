[user]
    name = Alex Trepca
    email = my@email.address
[alias]
    ch = checkout
    co = commit
    st = status
    b = branch
    p = pull
    d = diff
    dc = diff --cached
    l = log
    sh = show
    bl = blame
[core]
    pager = less -iR
    hooksPath = hooks
    editor = /usr/bin/vim
    autocrlf = input
[color]
    ui = true
[push]
    default = simple
[pull]
    rebase = true
[init]
    defaultBranch = main
